{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "tui",
  "tasks": {
    "clean": {
      "cache": false,
      "outputs": [],
      "inputs": []
    },
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["dist/**"],
      "env": ["XDG_CACHE_HOME"]
    },
    "check-types": {
      "dependsOn": ["^check-types"],
      "outputs": []
    },
    "lint": {
      "dependsOn": ["^lint"],
      "outputs": []
    },
    "test": {
      "dependsOn": ["build", "^test"],
      "outputs": []
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "version": {
      "dependsOn": ["build", "^version"],
      "outputs": []
    },
    "publish": {
      "dependsOn": ["version"],
      "outputs": []
    }
  }
}
